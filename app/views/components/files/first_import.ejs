    <!-- Manifest -->
    <link rel="manifest" href="/scripts/progressive/manifest.json" />
    <script>
        window.addEventListener("load",()=>{
            if("serviceWorker" in navigator){
                navigator.serviceWorker.register("/scripts/progressive/sw.js")
                    .then(reg => {
                        console.log("Service worker registered -->",reg);
                    }).catch(err=>{
                        console.log("Une erreur s'est produit : ",err)
                    });
            }
        })
    </script>
